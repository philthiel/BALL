BALL_INLINE 
Handle Object::getHandle () const
	
{
  return handle_;
}
  
BALL_INLINE 
Handle Object::getNextHandle()
	
{
  return (Object::global_handle_);
}
  
BALL_INLINE 
Handle Object::getNewHandle()
	
{
	Handle ret= Object::global_handle_++;
  return ret;
}
  
BALL_INLINE 
void Object::clear()
	
{
}

BALL_INLINE 
bool Object::operator == (const Object &object) const
	
{
  return (RTTI::isKindOf<Object>(&object) && (handle_ == object.handle_));
}

BALL_INLINE 
bool Object::operator != (const Object &object) const
	
{
  return !(*this == object);
}

BALL_INLINE 
bool Object::operator < (const Object &object) const
	
{
  return (RTTI::isKindOf<Object>(&object) && (handle_ < object.handle_));
}

BALL_INLINE 
bool Object::operator <= (const Object &object) const
	
{
  return !(object < *this);
}

BALL_INLINE 
bool Object::operator >= (const Object &object) const
	
{
  return !(*this < object);
}

BALL_INLINE 
bool Object::operator > (const Object &object) const
	
{
  return (object < *this);
}

BALL_INLINE
bool Object::isValid() const
	
{
  return true;
}

BALL_INLINE 
const Object& Object::operator = (const Object& /* object */)
	
{
	return *this;
}
